<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="helloWorldBean" class="njuics.demos.springbeans.HelloWorld">
		<constructor-arg ref="messagePrinterBean"></constructor-arg>
		<property name="message" value="Hello World Again!"></property>
	</bean>

	<bean id="messagePrinterBean" class="njuics.demos.springbeans.MessagePrinter">
	</bean>

	<bean id="logPrinterBean" class="njuics.demos.springbeans.LogPrinter">
	</bean>
</beans>
